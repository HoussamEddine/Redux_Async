{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "rules": {
        "no-console": [
            "off"
        ],
        "react/prefer-stateless-function": [
            "off"
        ],
        "react/jsx-indent": "off",
        "react/jsx-filename-extension": "off",
        "react/jsx-closing-tag-location": "off",
        "react/require-default-props": "off",
        "no-class-assign": "off"
    },
    "env": {
        "browser": true
    }
}